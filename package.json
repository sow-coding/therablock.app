{
  "name": "therablock",
  "version": "0.1.0",
  "author": "Swos",
  "description": "An app to avoid scrolling and wasting time on internet",
  "main": "main.js",
  "build": {
    "appId": "therablock.app",
    "productName": "Therablock",
    "win": {
      "target": "nsis",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "files": [
      "!dist/**/*",
      "!node_modules/**/*"
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "include": "build/installer.nsh"
    },
    "asar": true,
    "compression": "maximum",
    "extraResources": [
      {
        "from": "scripts/",
        "to": "scripts"
      }
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "electron-forge start",
    "lint": "next lint",
    "electron": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "dist": "electron-builder"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.5",
    "@mui/material": "^5.16.5",
    "@nextui-org/react": "^2.4.2",
    "@nextui-org/select": "^2.2.2",
    "@nextui-org/system": "^2.2.2",
    "@nextui-org/theme": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.1",
    "@supabase/ssr": "^0.4.0",
    "@supabase/supabase-js": "^2.44.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "electron-log": "^5.1.5",
    "electron-serve": "^2.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "framer-motion": "^11.2.12",
    "lucide-react": "^0.396.0",
    "next": "14.2.4",
    "node-schedule": "^2.1.1",
    "node-windows": "^1.0.0-beta.8",
    "ps-node": "^0.1.6",
    "react": "^18",
    "react-dom": "^18",
    "react-modal": "^3.16.1",
    "sudo-prompt": "^9.2.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1",
    "wait-on": "^7.2.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "electron": "^31.1.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
